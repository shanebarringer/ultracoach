{
  "name": "ultracoach",
  "version": "0.1.0",
  "description": "Ultra-marathon training management platform for coaches and runners",
  "private": true,
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "db:validate:schema": "bash supabase/scripts/validate_schema.sh --verbose",
    "db:validate:schema:fix": "bash supabase/scripts/validate_schema.sh --verbose --fix",
    "dev": "next dev -p 3001",
    "dev:local": "concurrently \"supabase start\" \"next dev -p 3001\"",
    "dev:db": "supabase start",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:db:seed": "NODE_ENV=test tsx scripts/seed-test-database.ts",
    "db:setup": "./supabase/scripts/setup_with_env.sh",
    "db:cli": "tsx scripts/db-cli.ts",
    "db:seed": "tsx scripts/seed-database.ts",
    "db:seed:secure": "tsx scripts/seed-local-secure.ts",
    "db:seed:comprehensive": "tsx scripts/comprehensive-seed.ts",
    "db:seed:onboarding": "tsx scripts/seed-onboarding-steps.ts",
    "db:reset": "./supabase/scripts/reset_database.sh",
    "db:fresh": "./supabase/scripts/reset_database.sh && tsx scripts/seed-database.ts",
    "db:fresh:comprehensive": "tsx scripts/comprehensive-seed.ts",
    "db:fix-passwords": "./supabase/scripts/fix_test_passwords.sh",
    "db:backup": "./supabase/scripts/backup_user_data.sh",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push --force",
    "db:studio": "drizzle-kit studio",
    "db:drop": "drizzle-kit drop",
    "prod:db:generate": "NODE_ENV=production drizzle-kit generate",
    "prod:db:migrate": "NODE_ENV=production drizzle-kit migrate",
    "prod:db:push": "NODE_ENV=production drizzle-kit push --force",
    "prod:db:studio": "NODE_ENV=production drizzle-kit studio",
    "db:connect": "export $(grep -v '^#' .env.local | xargs) && psql \"$DATABASE_URL\"",
    "db:query": "export $(grep -v '^#' .env.local | xargs) && psql \"$DATABASE_URL\" -c",
    "prod:db:reset": "tsx scripts/reset-production-database.ts",
    "prod:db:seed": "tsx scripts/seed-production-local-logic.ts",
    "prod:db:seed:secure": "tsx scripts/seed-production-secure.ts",
    "preview:db:seed": "tsx scripts/seed-production-local-logic.ts",
    "prod:db:fresh": "tsx scripts/reset-production-database.ts && tsx scripts/seed-production-local-logic.ts",
    "prod:db:query": "export $(grep -v '^#' .env.production | xargs) && psql \"$DATABASE_URL\" -c",
    "prod:db:status": "export $(grep -v '^#' .env.production | xargs) && psql \"$DATABASE_URL\" -c \"SELECT schemaname, tablename FROM pg_tables WHERE schemaname = 'public' ORDER BY tablename;\"",
    "prod:db:users": "export $(grep -v '^#' .env.production | xargs) && psql \"$DATABASE_URL\" -c \"SELECT id, email, role, created_at FROM \\\"user\\\" ORDER BY created_at;\"",
    "prod:db:keepalive": "NODE_ENV=production tsx scripts/database-keepalive.ts",
    "db:keepalive": "tsx scripts/database-keepalive.ts",
    "prepare": "command -v husky >/dev/null 2>&1 && husky || exit 0"
  },
  "dependencies": {
    "@better-auth/cli": "^1.2.12",
    "@heroicons/react": "^2.2.0",
    "@heroui/drawer": "^2.2.21",
    "@heroui/react": "^2.7.11",
    "@heroui/theme": "^2.4.17",
    "@hookform/resolvers": "^5.1.1",
    "@internationalized/date": "3.8.2",
    "@react-aria/i18n": "3.12.11",
    "@sentry/nextjs": "^10.1.0",
    "@supabase/supabase-js": "^2.50.5",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/better-sqlite3": "^7.6.13",
    "@types/pg": "^8.15.4",
    "@we-gold/gpxjs": "^1.1.0",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "better-auth": "^1.2.12",
    "better-sqlite3": "^12.2.0",
    "classnames": "^2.5.1",
    "commander": "^14.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.0",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "0.44.3",
    "framer-motion": "^12.23.3",
    "jotai": "^2.12.5",
    "kbar": "0.1.0-beta.48",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "papaparse": "^5.5.3",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "postgres": "^3.4.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.60.0",
    "resend": "^4.7.0",
    "sonner": "^2.0.7",
    "strava-v3": "^2.2.1",
    "tslog": "^4.9.3",
    "zod": "^4.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "^15.4.4",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/bcrypt": "^5.0.2",
    "@types/classnames": "^2.3.4",
    "@types/node": "^20",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.6.0",
    "code-inspector-plugin": "^1.2.2",
    "concurrently": "^9.2.0",
    "drizzle-seed": "^0.3.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
