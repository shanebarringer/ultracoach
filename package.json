{
  "name": "ultracoach",
  "version": "0.1.0",
  "description": "Ultra-marathon training management platform for coaches and runners",
  "private": true,
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "db:validate:schema": "bash supabase/scripts/validate_schema.sh --verbose",
    "db:validate:schema:fix": "bash supabase/scripts/validate_schema.sh --verbose --fix",
    "dev": "next dev -p 3001 --turbo",
    "dev:local": "concurrently \"supabase start\" \"next dev -p 3001 --turbo\"",
    "dev:db": "supabase start",
    "build": "next build",
    "build:charlie": "bash scripts/build-charlie.sh",
    "posthog:sourcemaps": "posthog-cli sourcemap inject --directory .next --project ultracoach && posthog-cli sourcemap upload --directory .next --delete-after",
    "build:production": "pnpm build && pnpm posthog:sourcemaps",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:local": "pnpm exec playwright test --reporter=list",
    "test:e2e:race-import": "pnpm exec playwright test race-import.spec.ts --reporter=list",
    "test:e2e:auth": "pnpm exec playwright test auth-flows.spec.ts --reporter=list",
    "test:e2e:ci": "CI=true pnpm exec playwright test --reporter=list",
    "test:e2e:race-import:ci": "CI=true pnpm exec playwright test race-import.spec.ts --reporter=list",
    "test:e2e:auth:ci": "CI=true pnpm exec playwright test auth-flows.spec.ts --reporter=list",
    "test:critical": "CI=true pnpm exec playwright test tests/auth.setup.ts tests/auth-coach.setup.ts tests/auth.spec.ts tests/e2e/race-import.spec.ts --max-failures=5 --reporter=list",
    "test:e2e:setup": "supabase start && echo 'Database ready for testing'",
    "test:e2e:ui": "playwright test --ui",
    "test:db:seed": "NODE_ENV=test tsx scripts/seed-test-database.ts",
    "db:setup": "./supabase/scripts/setup_with_env.sh",
    "db:cli": "tsx scripts/db-cli.ts",
    "db:seed": "tsx scripts/database/seed-database-unified.ts",
    "db:seed:legacy": "tsx scripts/seed-database.ts",
    "db:seed:onboarding": "tsx scripts/database/seed-onboarding-steps.ts",
    "db:reset": "supabase db reset --local",
    "db:fresh": "supabase db reset --local && tsx scripts/database/seed-database-unified.ts",
    "db:backup": "./supabase/scripts/backup_user_data.sh",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "supabase migration up --local",
    "db:push": "drizzle-kit push --force",
    "db:studio": "drizzle-kit studio",
    "db:drop": "drizzle-kit drop",
    "db:introspect": "drizzle-kit introspect",
    "prod:db:generate": "NODE_ENV=production drizzle-kit generate",
    "prod:db:migrate": "supabase db push --linked",
    "prod:db:push": "NODE_ENV=production drizzle-kit push --force",
    "prod:db:studio": "NODE_ENV=production drizzle-kit studio",
    "db:connect": "export $(grep -v '^#' .env.local | xargs) && psql \"$DATABASE_URL\"",
    "db:query": "export $(grep -v '^#' .env.local | xargs) && psql \"$DATABASE_URL\" -c",
    "prod:db:reset": "tsx scripts/database/reset-production-database.ts",
    "prod:db:seed": "tsx scripts/database/seed-production-local-logic.ts",
    "prod:db:seed:secure": "tsx scripts/database/seed-production-secure.ts",
    "preview:db:seed": "tsx scripts/database/seed-production-local-logic.ts",
    "prod:db:fresh": "tsx scripts/database/reset-production-database.ts && tsx scripts/database/seed-production-local-logic.ts",
    "prod:db:query": "export $(grep -v '^#' .env.production | xargs) && psql \"$DATABASE_URL\" -c",
    "prod:db:status": "export $(grep -v '^#' .env.production | xargs) && psql \"$DATABASE_URL\" -c \"SELECT schemaname, tablename FROM pg_tables WHERE schemaname = 'public' ORDER BY tablename;\"",
    "prod:db:users": "export $(grep -v '^#' .env.production | xargs) && psql \"$DATABASE_URL\" -c \"SELECT id, email, role, created_at FROM \\\"user\\\" ORDER BY created_at;\"",
    "prod:db:keepalive": "NODE_ENV=production tsx scripts/database-keepalive.ts",
    "db:keepalive": "tsx scripts/database-keepalive.ts",
    "prepare": "is-ci || husky"
  },
  "dependencies": {
    "@better-auth/cli": "^1.2.12",
    "@heroicons/react": "^2.2.0",
    "@heroui/drawer": "^2.2.21",
    "@heroui/react": "2.8.5",
    "@heroui/theme": "2.4.23",
    "@hookform/resolvers": "^5.1.1",
    "@internationalized/date": "3.8.2",
    "@react-aria/i18n": "3.12.11",
    "@supabase/supabase-js": "^2.50.5",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20",
    "@types/pg": "^8.15.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@upstash/redis": "^1.35.6",
    "@we-gold/gpxjs": "^1.1.0",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "better-auth": "^1.2.12",
    "better-sqlite3": "^12.2.0",
    "classnames": "^2.5.1",
    "commander": "^14.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.0",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.3",
    "framer-motion": "^12.23.3",
    "geist": "^1.5.1",
    "is-ci": "^4.1.0",
    "jotai": "^2.12.5",
    "kbar": "0.1.0-beta.48",
    "lucide-react": "^0.525.0",
    "motion": "^12.23.25",
    "next": "15.4.8",
    "nextstepjs": "^2.1.2",
    "papaparse": "^5.5.3",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "posthog-js": "^1.297.2",
    "posthog-node": "^5.13.2",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-hook-form": "^7.60.0",
    "resend": "^4.7.0",
    "sonner": "^2.0.7",
    "strava-v3": "^2.2.1",
    "tslog": "^4.9.3",
    "zod": "^4.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "^15.4.4",
    "@playwright/test": "^1.54.1",
    "@posthog/cli": "^0.5.13",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/bcrypt": "^5.0.2",
    "@types/classnames": "^2.3.4",
    "@types/papaparse": "^5.3.16",
    "@vitejs/plugin-react": "^4.6.0",
    "code-inspector-plugin": "^1.2.2",
    "concurrently": "^9.2.0",
    "drizzle-seed": "^0.3.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.8",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
